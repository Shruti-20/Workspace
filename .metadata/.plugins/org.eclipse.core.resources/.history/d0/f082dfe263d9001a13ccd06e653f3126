package pattern_program;

public class P2 {

	public static void main(String[] args) {
		int n=5;
	//first letter
	for(int i=0;i<n;i++)
	{
		for(int j=0; j<n;j++)
		{
			if((j==0 && i<5)|| (j==4 && i<5)|| (i==1 && (j==1 || j==3)) || (j==2 && i==2)) {
				System.out.print("* ");
			}
			else {
				System.out.print("  ");    
			}
		}
		System.out.print("_");
	}
	System.out.print("_");
	//second letter
	for(int i=0;i<n;i++)
	{
		for(int j=0; j<n;j++)
		{
			if((j==0 && (i>0 && i<4)) || (i==0 && (j>0 && j<4)) || (i==4 && (j>0 && j<4)) || (j==4 && (i>0 && i<4))) {
				System.out.print("* ");
			}
			else {
				System.out.print("  ");    
			}
		}
		System.out.print("_");
	}
	System.out.print("_");
	//third letter
	for(int i=0;i<n;i++)
	{
		for(int j=0; j<n;j++)
		{
			if((j==0 && i<5)|| (j==2 && (i==0 || i==4)) || (j==1 && (i==0 || i==4)) || (j==3 && (i>0&&i<4))) {
				System.out.print("* ");
			}
			else {
				System.out.print("  ");    
			}
		}
		System.out.print("_");
	}
	System.out.print("_");
	//fourth letter
	for(int i=0;i<n;i++)
	{
		for(int j=0; j<n;j++)
		{
			if((j==2 && i<5)|| (i==0 && j<5) || (i==4 && j<5)) {
				System.out.print("* ");
			}
			else {
				System.out.print("  ");    
			}
		}
		System.out.print("_");
	}
	System.out.print("_");
	//fifth letter
	for(int i=0;i<n;i++)
	{
		for(int j=0; j<n;j++)
		{
			if((j==0 && i<5) || (i==0 && j<4) || (i==2 && j<4) || (i==4  && j<4)) {
				System.out.print("* ");
			}
			else {
				System.out.print("  ");    
			}
		}
		System.out.print("_");
	}
	System.out.print("_");
	//sixth letter
	for(int i=0;i<n;i++)
	{
		for(int j=0; j<n;j++)
		{
			if((i==0 && (j==0 || j==4)) || (i==1 && (j==1 || j==3)) || (j==2 && i>1)) {
				System.out.print("* ");
			}
			else {
				System.out.print("  ");    
			}
		}
		System.out.print("_");
	}
	System.out.print("_");
	
	}

}